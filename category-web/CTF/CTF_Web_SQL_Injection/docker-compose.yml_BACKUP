version: "3.9"
services:
   web:
      build: .
      ports:
         - "8008:5000"
      volumes:
         - .:/code
      environment:
         FLASK_ENV: "development"
   db:
      image: "mysql/mysql-server:latest"
      restart: "unless-stopped"
      environment:
         MYSQL_ROOT_PASSWORD: "mysql-pass"
